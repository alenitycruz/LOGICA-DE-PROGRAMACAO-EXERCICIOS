algoritmo "parametroprocedimento"
var
   x, y : Inteiro
   procedimento soma(a, b : inteiro)
   inicio
   escreval("Recebi o valor ", a)
   escreval("Recebi o valor ", b)
   escreval("A soma entre ", a, " e ", b, " é igual a ", a+b)
   fimprocedimento
inicio
      x <- 5
      y <- 3
      soma(x,y)

fimalgoritmo