algoritmo "parammetroprocedimento2"
var
   x, y : Inteiro
   procedimento Soma(a, b : Inteiro)
   inicio
   escreval("A soma entre os valores é igual a ", a+b)
   fimprocedimento
inicio
      Escreval("Digite um valor: ")
      leia(x)
      Escreval("Digite outro valor: ")
      leia(y)
      Soma(x,y)

fimalgoritmo